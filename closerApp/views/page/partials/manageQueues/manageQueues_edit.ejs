<div class="editContainer" style="display:none;">
	<form>
		<label for="queueName">Name:</label>
		<input class="text" type="text" name="name" value="<%= data.getName() %>" />

		<label for="queueDescription">Description:</label>
		<input class="text" type="text" name="description" value="<%= data.getDescription() %>" />

		<label for="queueActive">Active:</label>
		<select name="active">
			<option value="true" <% if ( data.getIsActive() == true ) { %> selected="selected" <% } %>>Yes</option>
			<option value="false" <% if ( data.getIsActive() == false ) { %> selected="selected" <% } %>>No</option>
		</select>
	</form>
</div>
<div class="editButtons" style="display:none;">
	<input type="button" name="saveButton" value="Save" title="Save Changes" class="saveButton" <%= data %> />
</div>
